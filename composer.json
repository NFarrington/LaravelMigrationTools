{
    "name": "nfarrington/laravel-migration-tools",
    "description": "Migration tools for Laravel. Adds additional functionality to Laravel migrations.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Neil Farrington",
            "email": "me@neilfarrington.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5,<5.6",
        "illuminate/console": "^5.5,<5.6",
        "illuminate/database": "^5.5,<5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "NFarrington\\LaravelMigrationTools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NFarrington\\LaravelMigrationTools\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NFarrington\\LaravelMigrationTools\\MigrationToolsServiceProvider"
            ]
        }
    }
}
